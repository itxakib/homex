{
  "expo": {
    "name": "homex",
    "slug": "homex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aqibnewpass.homex",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs camera access to capture CNIC images."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0b0f2b"
      },
      "package": "com.aqibnewpass.homex",
      "permissions": ["android.permission.CAMERA"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "994d0f28-754c-4740-85d4-bc0b5a96ce7b"
      }
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontencom.googleusercontent.apps.655253277613-9fn6fndd5nbjj821jidrst22j0qtrfr2"
        }
      ],
      "react-native-vision-camera",
      [
        "expo-camera",
        {
          "cameraPermission": "This app needs camera access to capture CNIC images."
        }
      ],
      "expo-dev-client"
    ]
  }
}